{
  "description": "Create and edit page objects for Selenium testing.",
  "manifest_version": 2,
  "author": "Chris Merrill",
  "name": "Page Objects Builder",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "cookies",
    "<all_urls>"
  ],
  "applications": {
    "gecko": {
      "id": "PageObjectsBuilder-1.0@ide4selenium.com"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}